<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="roadSegmentCollection">
		<xs:complexType>			
			<xs:sequence>
				<xs:element name="roadSegment" maxOccurs="unbounded">
					<xs:complexType mixed="true">
						<xs:sequence>
							<xs:element name="FNODE" type="xs:integer"/>
							<xs:element name="TNODE" type="xs:integer"/>
							<xs:element name="LENGTH" type="xs:decimal"/>
							<xs:element name="TYP" type="xs:integer"/>
							<xs:element name="VEJNAVN" type="xs:string"/>
							<xs:element name="FROMLEFT" type="xs:integer"/>
							<xs:element name="TOLEFT" type="xs:integer"/>
							<xs:element name="FROMRIGHT" type="xs:integer"/>
							<xs:element name="TORIGHT" type="xs:integer"/>
							<xs:element name="FROMLEFT_BOGSTAV" type="xs:string"/>
							<xs:element name="TOLEFT_BOGSTAV" type="xs:string"/>
							<xs:element name="FROMRIGHT_BOGSTA" type="xs:string"/>
							<xs:element name="TORIGHT_BOGSTAV" type="xs:string"/>
							<xs:element name="V_POSTNR" type="xs:integer"/>
							<xs:element name="H_POSTNR" type="xs:integer"/>
							<xs:element name="FRAKOERSEL" type="xs:integer"/>
							<xs:element name="ZONE" type="xs:integer"/>
							<xs:element name="SPEED" type="xs:integer"/>
							<xs:element name="DRIVETIME" type="xs:decimal"/>
							<xs:element name="ONE_WAY" type="xs:string"/>
							<xs:element name="F_TURN" type="xs:string"/>
							<xs:element name="T_TURN" type="xs:string"/>
							<xs:element name="TJEK_ID" type="xs:integer"/>
						</xs:sequence>				
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>	
	</xs:element>
	
</xs:schema>
